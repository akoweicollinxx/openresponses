    },
    "command": {
      "items": {
        "type": "string"
      },
      "type": "array",
      "description": "The command to execute, expressed as an array of strings."
    },
    "timeout_ms": {
      "anyOf": [
        {
          "type": "integer",
          "description": "The maximum time to allow for the command, in milliseconds."
